{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "http",
  "title": "HTTP",
  "description": "HTTP Requests Manager",
  "icon": "extension_icon.png",
  "author": "negasus",
  "license": "MIT",
  "categories": ["Developer Tools", "Productivity"],
  "commands": [
    {
      "name": "http",
      "title": "HTTP",
      "description": "Run HTTP requests",
      "mode": "view"
    },
    {
      "name": "profiles",
      "title": "HTTP Profiles",
      "subtitle": "Manage HTTP profiles",
      "description": "Manage HTTP profiles",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.0",
    "@raycast/utils": "^1.14.0",
    "axios": "^1.6.8",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
